INSERT INTO qas VALUES(1,1,1,'马上就能显示了。。','間もなくアップ反映されます。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_1.mp3','2019-05-26 07:24:11.813953','2019-05-26 23:37:03.089036');
INSERT INTO qas VALUES(2,1,2,'顺利更新了。','無事アップされました。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_2.mp3','2019-05-26 07:24:11.813953','2019-05-26 07:24:11.813953');
INSERT INTO qas VALUES(3,1,3,'好嘞。','よしっ','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_3.mp3','2019-05-26 07:24:11.813953','2019-05-26 07:24:11.813953');
INSERT INTO qas VALUES(4,1,4,'眼镜商人的活动网站，顺利更新完毕了。','眼鏡商人さんのキャンペーンサイト、無事アップいたしました。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_4.mp3','2019-05-26 07:24:11.813953','2019-05-26 07:24:11.813953');
INSERT INTO qas VALUES(5,1,5,'大家也要踊跃报名哦。','皆さんも、ご応募よろしくお願いします。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_5.mp3','2019-05-26 07:24:11.813953','2019-05-26 07:24:11.813953');
INSERT INTO qas VALUES(6,1,6,'那我们开始进行最后的检查吧。','じゃあ　最終チェック始めますか','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_6.mp3','2019-05-26 07:24:11.813953','2019-05-26 07:24:11.813953');
INSERT INTO qas VALUES(7,1,7,'好。','はい。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_7.mp3','2019-05-26 07:24:11.813953','2019-05-26 07:24:11.813953');
INSERT INTO qas VALUES(8,1,8,'收到。','了解です。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_8.mp3','2019-05-26 07:24:11.813953','2019-05-26 07:24:11.813953');
INSERT INTO qas VALUES(9,1,9,'我用手机报完名了。','スマホ応募できました。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_9.mp3','2019-05-26 07:24:11.813953','2019-05-26 07:24:11.813953');
INSERT INTO qas VALUES(10,1,10,'SNS分享呢？','SNSシェアは？','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_10.mp3','2019-05-26 07:24:11.813953','2019-05-26 07:24:11.813953');
INSERT INTO qas VALUES(11,2,11,'没问题。','OKです。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_11.mp3','2019-05-26 07:24:17.956454','2019-05-26 07:24:17.956454');
INSERT INTO qas VALUES(12,2,12,'承蒙您的关照，我是Net Heroes的东山。','お世話になっております　ネットヒーローズの東山です。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_12.mp3','2019-05-26 07:24:17.956454','2019-05-26 07:24:17.956454');
INSERT INTO qas VALUES(13,2,13,'网站已顺利开通，并且已经确认没有问题。','リリース完了し　問題無い事確認できました。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_13.mp3','2019-05-26 07:24:17.956454','2019-05-26 07:24:17.956454');
INSERT INTO qas VALUES(14,2,14,'是的。','はい。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_14.mp3','2019-05-26 07:24:17.956454','2019-05-26 07:24:17.956454');
INSERT INTO qas VALUES(15,2,15,'好的，谢谢。','はい　ありがとうございます。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_15.mp3','2019-05-26 07:24:17.956454','2019-05-26 07:24:17.956454');
INSERT INTO qas VALUES(16,2,16,'我先挂了。','失礼します。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_16.mp3','2019-05-26 07:24:17.956454','2019-05-26 07:24:17.956454');
INSERT INTO qas VALUES(17,2,17,'客户也说没问题，','クライアントのOKも出ました。','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_17.mp3','2019-05-26 07:24:17.956454','2019-05-26 07:24:17.956454');
INSERT INTO qas VALUES(18,2,18,'结束了。','終わり！','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_18.mp3','2019-05-26 07:24:17.956454','2019-05-26 07:24:17.956454');
INSERT INTO qas VALUES(19,2,19,'大家辛苦了！','お疲れ様でした！','https://s3.us-east-2.amazonaws.com/vocabrary-app/watashi-teijide-kaerimasu_1_19.mp3','2019-05-26 07:24:17.956454','2019-05-26 07:24:17.956454');
